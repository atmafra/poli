<neural network>
  neural network name: LVQ teste
  neural network extension: LVQ
</neural network>

<lvq extension>
  learning rate function: Exponential Decay
  learning rate function parameters: 2 0.1 1000.000000
  algorithm: LVQ-3
  activation metric: Euclidean
  output classes: 14
  units per class: 3
  window width: 0.20000
  LVQ3 epsilon: 0.30000
</lvq extension>

<layer>
  layer name: Camada de Entrada
  layer index: 1
  layer class: Input
  layer default units: 256
  activation class: Pass-through
</layer>

<layer>
  layer name: Camada de Saída
  layer index: 2
  layer class: Output
  layer default units: 42
  activation class: Linear
  activation parameters: 2 1.0 0.0
</layer>

<layer connection>
  origin layer: 1
  destination layer: 2
  initial weight: 0.0
  initialization method: Gaussian Random
  initialization parameters: 2 0.0 0.05
</layer connection>
